SubProceso menu()
	Definir nombre Como Caracter;
	Definir trofeos, long, opc, i, u como entero;
	Definir items Como Caracter;
	Dimension items[100];
	u = 1;
	para i = 1 Hasta 100 Hacer
		items[i] = "";
	FinPara
	nombre = " ";
	hacer
		Escribir "Ingrese su usuario" Sin Saltar;
		leer nombre;
	Hasta Que nombre <> " ";
	trofeos = 0;
	//#1 - INVOCAR LA PANTALLA DE CARGA.
	pantallaCarga(opc);
	si opc = 1 Entonces
		Hacer
			seleccion(opc);				//Cada vez que se termine una accion, volvera a preguntar por la posicion a la que quiere acceder.
			segun opc Hacer
				caso 1: 
					problema(trofeos);
				caso 2:
					tienda(trofeos, items, u);
				Caso 3:
					perfil(trofeos, nombre, items);
				caso 23:
					admin();
				Caso 123456789:
					trofeos = trofeos + 10000;
			FinSegun
		Mientras Que opc <> 4;
	FinSi	//EN CASO DE QUE LA OPCION DEL USUARIO HAYA SIDO SALIR ENTONCES NOS SALTARA ESTE BLOQUE DE CODIGOS.
	
FinSubProceso

//#1 - INVOCAR LA PANTALLA DE CARGA.
SubProceso pantallaCarga(opc Por Referencia)
	Definir final, i, j Como Entero;
	Definir marcos, title, marco, titulo, letra Como Caracter;
	dimension marco[100], titulo[100];
	
	Limpiar Pantalla
	marcos = "=======================================";
	title = "   WELCOME TO HELLO WORLD - GAME 1.9   ";
	final = Longitud(marcos);
	para i = 1 Hasta final Hacer
		letra = Subcadena(marcos, i, i);		//GUARDARA NUESTRO MARCO Y TITULO PARA HACER UN EFECTO DE ANIMACION.
		marco[i] = letra;
		letra = Subcadena(title, i, i);
		titulo[i] = letra;
	FinPara
	
	//	########################### 	Entrada de ANIMACION	#######################
	
	Para i = 1 Hasta final Hacer	
		Escribir Sin Saltar marco[i];
		Esperar 20 Milisegundos;
	FinPara	;	escribir "";
	Para i = 1 Hasta final Hacer
		Escribir Sin Saltar titulo[i];
		Esperar 80 Milisegundos;
	FinPara	;	escribir "";
	Para i = 1 Hasta final Hacer
		Escribir Sin Saltar marco[i];
		Esperar 20 Milisegundos;
	FinPara	;	escribir "";
	
	Hacer
		Borrar Pantalla		//Menu para inicializar la jugada o simplemente salir.
		Escribir "=======================================";
		Escribir "   WELCOME TO HELLO WORLD - GAME 1.9   ";
		Escribir "=======================================";
		
		Escribir "Choose an Option:";
		Escribir "(1) Play";
		Escribir "(2) Exit";
		Escribir Sin Saltar "Opc: ";
		leer opc;
	Mientras Que opc <> 1 y opc <> 2
	
FinSubProceso

