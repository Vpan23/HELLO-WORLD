SubProceso problema(trofeos Por Referencia)
	Definir num1, num2, operacion, vidaEnemigo, vidaUsuario  Como entero;
	Definir respuesta, resultado como real;
	Definir pausa Como caracter;
	vidaEnemigo = 3;
	vidaUsuario = 3;
	retornoEmparejando();
	//trofeos = 1000;
	Hacer
		resultado = 0
		num1 = Aleatorio(0,9);
		num2 = Aleatorio(0,9);
		operacion = Aleatorio(1,4);
		//LA OPERACION CAMBIARA SEGUN SEA EL NUMERO QUE LE SEA OTORGADA.
		//1. - Suma.
		//2. - Resta.
		//3. - Multiplicacion.
		//4. - Division.
		Limpiar Pantalla;
		Escribir "";
		Escribir "(You)    (Enemy)";
		Escribir " []        []";
		Escribir " ",vidaUsuario,"         ",vidaEnemigo;
		Escribir "";
		Escribir "";
		tocapara();
		
		Limpiar Pantalla;
		si trofeos < 300 Entonces
			Escribir "Nivel Basico "
		FinSi
		si trofeos > 300 y trofeos < 600 Entonces
			Escribir "Nivel Media ";
		sino 
			si trofeos > 600 y trofeos < 900 Entonces
				Escribir "Nivel Experto ";
			SiNo
				si trofeos > 900 Entonces
					Escribir "Nivel Dios ";
				FinSi
			FinSi
		FinSi
		Escribir "RESUELVA:";
		segun operacion Hacer
			caso 1: 
				Escribir num1," + ",num2," = ";
				resultado = num1 + num2;
			caso 2: 
				Escribir num1," - ",num2," = ";
				resultado = num1 - num2;
			caso 3: 
				si trofeos > 300 y trofeos < 600 Entonces
					num1 = Aleatorio(1,99);
					num2 = Aleatorio(1,99);
				sino 
					si trofeos > 600 y trofeos < 900 Entonces
						num1 = Aleatorio(1,999);
						num2 = Aleatorio(1,999);
					SiNo
						si trofeos > 900 Entonces
							num1 = Aleatorio(1,9999);
							num2 = Aleatorio(1,9999);
						FinSi
					FinSi
				FinSi
				Escribir num1," X ",num2," = ";
				resultado = num1 * num2;
			caso 4: 
				Hacer
					num1 = Aleatorio(1,9);
					num2 = Aleatorio(1,9);
					si trofeos > 300 y trofeos < 600 Entonces
						num1 = Aleatorio(1,99);
						num2 = Aleatorio(1,99);
					sino 
						si trofeos > 600 y trofeos < 900 Entonces
							num1 = Aleatorio(1,999);
							num2 = Aleatorio(1,999);
						SiNo
							si trofeos > 900 Entonces
								num1 = Aleatorio(1,9999);
								num2 = Aleatorio(1,9999);
							FinSi
						FinSi
					FinSi
					
				Hasta Que num1 % num2 = 0;		//Cambiara ambos numeros hasta que el primero sea mayor que el segundo y que sean mayores a 0.
				
				Escribir num1," / ",num2," = ";
				resultado = num1 / num2;
		FinSegun
		leer respuesta;
		si respuesta = resultado Entonces
			vidaEnemigo = vidaEnemigo - 1;
		SiNo
			vidaUsuario = vidaUsuario - 1;
		FinSi
	Hasta Que vidaEnemigo = 0 o vidaUsuario = 0 
	Limpiar Pantalla;
	si vidaEnemigo = 0 Entonces
		Escribir "Has ganado! +30 Trofeos";
		trofeos = trofeos + 300;
	SiNo
		Escribir "Has perdido! -30 Trofeos";
		trofeos = trofeos - 30;
		si trofeos < 0 Entonces
			trofeos = 0;
		FinSi
	FinSi
	Escribir "Tienes: ", trofeos, " Trofeos";
	tocapara();
FinSubProceso


